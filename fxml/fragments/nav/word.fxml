      <CheckBox mnemonicParsing="false" />
      <Label maxWidth="1.7976931348623157E308" minWidth="50.0" text="Mot" textFill="WHITE">
         <font>
            <Font name="Inter Regular" size="12.0" />
         </font>
      </Label>
      <Label minWidth="50.0" styleClass="description" text="Mot ASCII" textFill="WHITE" />
      <Label maxWidth="1.7976931348623157E308" minWidth="40.0" text="Type" textFill="#00ccff" HBox.hgrow="ALWAYS" />
